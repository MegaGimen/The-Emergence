<?xml version='1.0' encoding='utf-8'?>
<interface version="1.4" author="PygubuDesigner 0.39.3">
  <project>
    <settings />
    <customwidgets />
  </project>
  <object class="tk.Tk" id="开盒" named="True">
    <containerlayout manager="grid">
      <property type="col" id="0" name="weight">1</property>
      <property type="row" id="0" name="weight">1</property>
    </containerlayout>
    <child>
      <object class="ttk.Frame" id="frame1">
        <layout manager="grid">
          <property name="columnspan">1</property>
          <property name="rowspan">1</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label1">
            <property name="cursor">based_arrow_down</property>
            <property name="font">TkDefaultFont</property>
            <property name="image">background.png</property>
            <property name="justify">center</property>
            <property name="state">normal</property>
            <layout manager="pack">
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="retrieve" named="True">
            <property name="text" translatable="yes">查询</property>
            <bind sequence="&lt;ButtonPress&gt;" handler="showRetrieve" add="" />
            <layout manager="pack">
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="write" named="True">
            <property name="text" translatable="yes">写入</property>
            <bind sequence="&lt;ButtonPress&gt;" handler="showWrite" add="" />
            <layout manager="pack">
              <property name="side">top</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Toplevel" id="writeWindow" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="writeFrame" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="columnspan">1</property>
          <property name="row">4</property>
          <property name="rowspan">1</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label5">
            <property name="text" translatable="yes">是谁</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label6">
            <property name="text" translatable="yes">和谁</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label7">
            <property name="text" translatable="yes">关系</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="writeWho" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="writeWithWhom" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="writeRelation" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Radiobutton" id="radioPrivate" named="True">
            <property name="text" translatable="yes">私密</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Radiobutton" id="radioNotPrivate" named="True">
            <property name="text" translatable="yes">不私密</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="writeButton" named="True">
            <property name="text" translatable="yes">确定</property>
            <bind sequence="&lt;ButtonPress&gt;" handler="writeButtonHandler" add="" />
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Toplevel" id="retrieveWindow" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="retrieveFrame" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="row">2</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label12">
            <property name="text" translatable="yes">查谁</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label13">
            <property name="text" translatable="yes">和谁</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="retrieveWho" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="retrieveWithWhom" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="retrieveButton" named="True">
            <property name="text" translatable="yes">查询</property>
            <bind sequence="&lt;ButtonPress&gt;" handler="retrieveButtonHandler" add="" />
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
